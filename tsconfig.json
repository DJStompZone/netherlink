{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "*": [
                "decl/*"
            ],
            "netherlink/*": [
                "lib/*"
            ]
        },
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strict": true,
        "target": "ES6",
        "module": "CommonJS",
        "sourceMap": true
    }
}
